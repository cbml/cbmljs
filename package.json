{
  "name": "cbml",
  "version": "0.1.0",
  "description": "CBML Parser",
  "homepage": "https://github.com/cbml/cbmljs",
  "author": {
    "name": "zswang",
    "url": "http://weibo.com/zswang"
  },
  "repository": "https://github.com/cbml/cbmljs.git",
  "license": "MIT",
  "main": "cbml.js",
  "files": [
    "cbml.js",
    "cli.js"
  ],
  "keywords": [
    "cbml",
